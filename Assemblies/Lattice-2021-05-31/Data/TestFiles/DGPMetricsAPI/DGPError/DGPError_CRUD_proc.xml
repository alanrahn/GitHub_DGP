					<Prm>
						<Name>ErrData</Name>
						<Val>
							<![CDATA[Test error data]]>
						</Val>
					</Prm>
				</PList>
			</Meth>
			<RList>
				<Result>
					<RName>DGPError.New.base_DEFAULT</RName>
					<ExpRCode>OK</ExpRCode>
					<ExpDType>TEXT</ExpDType>
					<ExpRVal></ExpRVal>
					<ValMatch></ValMatch>
					<VarName>NEWGID</VarName>
				</Result>
			</RList>
