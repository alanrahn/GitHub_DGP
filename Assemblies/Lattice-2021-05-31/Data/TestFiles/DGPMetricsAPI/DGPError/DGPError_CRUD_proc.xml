			<TMExpAuthCode>OK</TMExpAuthCode>
			<Meth>
				<MName>DGPError.New.base</MName>
				<PList>
					<Prm>
						<Name>UserName</Name>
						<Val>
							<![CDATA[AutoTestUser]]>
